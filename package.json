{
  "name": "vue-hover-mask",
  "description": "vue遮罩组件, 当hover上显示遮罩",
  "version": "1.0.0",
  "private": false,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue build -t lib --name vue-hover-mask -d ./dist/ ./src/index.js",
    "lint": "vue-cli-service lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dream2023/vue-hover-mask"
  },
  "main": "dist/vue-hover-mask.umd.min.js",
  "homepage": "https://github.com/dream2023/vue-hover-mask/",
  "keywords": [
    "vue-hover-mask",
    "vue mask",
    "遮罩组件",
    "遮罩",
    "vue 遮罩"
  ],
  "dependencies": {},
  "devDependencies": {
    "core-js": "^2.6.5",
    "vue": "^2.6.10",
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
